name: house_prices_training

conda_env: conda.yaml

entry_points:
  main:
    parameters:
      max_iter: {type: int, default: 300}
      max_depth: {type: int, default: 10}
      learning_rate: {type: float, default: 0.1}
    command: "python modelling.py --max_iter {max_iter} --max_depth {max_depth} --learning_rate {learning_rate}"
